<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
  <!--
    OWASP Dependency Check Suppressions
    
    Add suppressions for false positives or accepted risks.
    Review regularly and remove when dependencies are updated.
  -->
  
  <!-- Example: Suppress false positive for test dependencies -->
  <!--
  <suppress>
    <notes>JUnit is only used for testing, not in production</notes>
    <cve>CVE-XXXX-XXXXX</cve>
  </suppress>
  -->
  
  <!-- Suppress Paper API issues (we don't bundle it) -->
  <suppress>
    <notes>Paper API is provided by the server, not bundled</notes>
    <gav regex="true">^io\.papermc\.paper:paper-api:.*$</gav>
  </suppress>
  
</suppressions>
